{
    "manifest_version": 3,
    "name": "URL Modifier",
    "version": "1.0",
    "background":{
      "service_worker":"background.js"
    },
    "description": "Modify base URL for specific website",
    "permissions": ["storage", "webNavigation","declarativeNetRequest","webRequest"],
    "action": {
      "default_popup": "popup.html"
    },
   "host_permissions":[
    "*://chat.openai.com/*","*://www.google.com/*","*://chat.openai.com/backend-api/conversation/*","*://api-gw-prod-demo.knostic.cloud/*","data:image/*"
   ]
  }
  