{
    "manifest_version": 3,
    "name": "URL Modifier",
    "version": "1.0",
    "description": "Modify base URL for specific website",
    "permissions": ["storage", "webNavigation","declarativeNetRequest"],
    "action": {
      "default_popup": "popup.html"
    },
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "redirectRule",
          "enabled": true,
          "path": "redirectRule.json"
        }
      ]  
    },
   "host_permissions":[
    "*://chat.openai.com/*","*://www.google.com/*","*://chat.openai.com/backend-api/conversation/*"
   ]
  }
  